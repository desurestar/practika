package ru.inversion.jinvfc.entity;

/**
 *
 * @author psh
 */
public enum PojoAnnotationMode { TABLE, NAMEDNATIVEQUERY, NONE;
    
    @Override
    public String toString () 
    {        
        if (this == TABLE)
           return "Table";
        if (this == NAMEDNATIVEQUERY)
           return "NamedNativeQuery";
        if (this == NONE)
           return "";
        
        return null;
    }    
}
