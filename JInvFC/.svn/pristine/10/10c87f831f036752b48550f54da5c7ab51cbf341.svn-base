package ru.inversion.jinvfc.entity;

/**
 *
 * @author psh
 */
public enum ColumnPlace { POJO, TABLE, GRIDPANE, NONE;
    
    @Override
    public String toString () 
    {        
        if (this == POJO)
           return "Pojo";
        if (this == TABLE)
           return "Pojo+JInvTable";
        if (this == GRIDPANE)
           return "Pojo+GridPane";
        if (this == NONE)
           return "";
        
        return null;
    }
}
